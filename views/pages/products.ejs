          var height = parseInt(
            $(this).find(".dynamic-wrap").css("height").replace("px", "")
          );
          var maxHeight = parseInt($(this).attr("data-maxheight"));
          if (height < maxHeight) {
            $(this).find(".js-dynamic-show-hide").hide();
          }
        });
      });
